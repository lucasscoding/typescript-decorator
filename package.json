{
  "name": "typescript-decorator",
  "version": "0.0.4",
  "description": "",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc --declaration",
    "start": "node dist/index.js",
    "watch": "nodemon -e ts -w ./src -x npm run dev",
    "dev": "nodemon --watch 'src/' --exec 'npm run build && ts-node src/index.ts' -e ts"
  },
  "dependencies": {
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "author": "Lucas Lopes",
  "license": "MIT",
  "_moduleAliases": {
    "@": "dist"
  }
}
